{
  "title": "Services",
  "editable": true,
  "schemaVersion": 42,
  "panels": [
    {
      "id": 1,
      "title": "Service Status",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red",
                  "index": 0
                }
              }
            },
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "UP",
                  "color": "green",
                  "index": 1
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          },
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["last"],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "probe_success{job=\"blackbox\"}",
          "legendFormat": "{{ instance }}",
          "instant": true,
          "datasource": {
            "uid": "prometheus-default"
          }
        }
      ]
    },
    {
      "id": 2,
      "title": "Response Time (ms)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisLabel": "",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "probe_duration_seconds{job=\"blackbox\"} * 1000",
          "legendFormat": "{{ instance }}",
          "datasource": {
            "uid": "prometheus-default"
          }
        }
      ]
    },
    {
      "id": 3,
      "title": "SSL Certificate Expiry",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 7,
                "color": "orange"
              },
              {
                "value": 30,
                "color": "green"
              }
            ]
          },
          "unit": "d",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": ["last"],
          "fields": "",
          "values": false
        },
        "textMode": "value_and_name"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(probe_ssl_earliest_cert_expiry{job=\"blackbox\"} - time()) / 86400",
          "legendFormat": "{{ instance }}",
          "instant": true,
          "datasource": {
            "uid": "prometheus-default"
          }
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}
